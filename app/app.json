{
  "expo": {
    "name": "HealPath",
    "slug": "healpath",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#e3f2fc"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.healpath.app",
      "infoPlist": {
        "NSCameraUsageDescription": "HealPath needs camera access for profile photos",
        "NSPhotoLibraryUsageDescription": "HealPath needs photo library access for profile photos",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#e3f2fc"
      },
      "package": "com.healpath.app",
      "permissions": [
        "CAMERA",
        "CAMERA"
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "lanzkrish",
    "extra": {
      "eas": {
        "projectId": "f49c4135-9e97-4a24-90ea-275c901c356c"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f49c4135-9e97-4a24-90ea-275c901c356c"
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#007AFF"
        }
      ]
    ]
  }
}